{
  "name": "Exchange Rates",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Extension to  convert currencies quickly",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"]
}